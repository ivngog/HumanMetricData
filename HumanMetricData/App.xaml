<Application x:Class="HumanMetricData.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HumanMetricData"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ImageBrush x:Key="btn_Close" ImageSource="Images/Icons/btn_Close.png"/>
        <ImageBrush x:Key="MainWin_Img" ImageSource="Images/MainWFont/1.jpg"/>
        <ImageBrush x:Key="btn_Search" ImageSource="Images/Icons/search.png"/>
        <ImageBrush x:Key="btn_Add" ImageSource="Images/Icons/Add.png"/>
        <ImageBrush x:Key="btn_Edit" ImageSource="Images/Icons/Edit.png"/>
        <ImageBrush x:Key="btn_Delete" ImageSource="Images/Icons/Delete.png" Stretch="UniformToFill"/>
        <ImageBrush x:Key="btn_ToWord" ImageSource="Images/Icons/btn_SendToW.png"/>
        <ImageBrush x:Key="App_Icon" ImageSource="Images/Icons/1lerc2tu.png"/>
        <ImageBrush x:Key="Main_Picture" ImageSource="Images/Pictures/yzluonhq.jpg"/>
    </Application.Resources>
</Application>
